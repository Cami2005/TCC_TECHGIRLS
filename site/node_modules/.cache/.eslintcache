n        } catch (err) {\r\n            ref.current.complete();\r\n            setCarregando(false);\r\n            if(err.response.status === 401){\r\n                setErro(err.response.data.erro);\r\n        }\r\n    }\r\n\r\n}\r\n    return(\r\n        <main className=\"login\">\r\n            <LoadingBar color='linear-gradient(270deg, #FF2B5E 35.42%, #521FAF 50.83%, #2BFFBF 64.58%)' ref={ref}/>\r\n\r\n            <div className=\"administracao\">\r\n                 <h1>ADMINISTRAÇÃO</h1>\r\n\r\n            <div>\r\n                <Inputs type='text' value={email} onChange={e => setEmail(e.target.value)} placeholder=\"email\" classe=\"componente-input\" img=\"img\" icon=\"../images/usuario1.png\"/>\r\n                <Inputs type='password' value={senha} onChange={e => setSenha(e.target.value)} placeholder=\"Senha\" classe=\"componente-input\" img=\"img\" icon=\"../images/senha1.png\"/>\r\n            </div>\r\n\r\n            <div className=\"flex-column\">\r\n                <button onClick={Acessar}  className=\"acessar\" disabled={carregando}>ACESSAR</button>\r\n                <Link to='/Menu' className=\"voltar\" >Voltar</Link>\r\n            </div>\r\n\r\n                <div className='credenciais'>\r\n                    {erro}\r\n                </div>\r\n            </div>\r\n\r\n        </main>\r\n    )\r\n\r\n}","C:\\Users\\Lenovo\\TECH GIRLS\\TCC_TECHGIRLS-1\\site\\src\\pages\\Administrador\\Menu\\index.js",[],[],"C:\\Users\\Lenovo\\TECH GIRLS\\TCC_TECHGIRLS-1\\site\\src\\pages\\Administrador\\Produtos\\index.js",[],[],"C:\\Users\\Lenovo\\TECH GIRLS\\TCC_TECHGIRLS-1\\site\\src\\pages\\Administrador\\AddProduto\\index.js",["68","69"],[],"\r\nimport MenuAdmin from \"../../../components/pagAdm.js\";\r\nimport \"./index.scss\";\r\nimport \"../../../common/common.scss\"\r\nimport { useState } from \"react\";\r\nimport { CadastrarCor, CadastrarPoduto } from '../../../API/CadProduto.js';\r\n\r\nexport default function Index() {\r\n    const [nome, setNome] = useState('');\r\n    const [descricao, setDescricao] = useState('');\r\n    const [preco, setPreco] = useState('');\r\n    const [disponivel, setDisponivel] = useState(false);\r\n\r\n    ///////////////////////////\r\n    const[cor, setCor] = useState('');\r\n\r\n    async function SalvarCor(){\r\n        try {\r\n            const resp= await CadastrarCor(cor);\r\n            alert('cor cadastrada')\r\n        } catch (error) {\r\nalert('ERR')\r\n        }\r\n    }\r\n    \r\n    async function Adicionar(){\r\n        try {\r\n            const precoProduto = Number(preco.replace(',', '.'));\r\n\r\n            const r = await CadastrarPoduto (nome, descricao, precoProduto, disponivel)\r\n            alert('Produto cadastrado com sucesso')\r\n        } catch (err) {\r\n           alert(err.response.data.erro);\r\n        }\r\n    }\r\n\r\n    \r\n    return (\r\n        <main className=\"inserirProduto\">\r\n\r\n                <MenuAdmin logo='../../../images/logoAdmin.png'/>\r\n\r\n                <div className=\"informacoes-produto\">\r\n\r\n                    <h1> Adicionar Produto </h1>\r\n\r\n                    <div className=\"inserir\">\r\n\r\n                        <div className=\"div1\">\r\n                            <div className=\"flex-column\">\r\n                                <label> Código do Produto </label>\r\n                                <input className=\"input\"/>\r\n                            </div>\r\n\r\n                            <div className=\"flex-column\">\r\n                                <label> Nome do Produto </label>\r\n                                <input type=\"text\" value={nome} onChange={e=> setNome(e.target.value)} className=\"input\"/>\r\n                            </div>\r\n\r\n                            <div className=\"flex-column\">\r\n                                <label> Descrição do Produto </label>\r\n                                <textarea type=\"text\" value={descricao} onChange={e=> setDescricao(e.target.value)} maxLength={100} className=\"textarea\"/>\r\n                            </div>\r\n\r\n                            \r\n\r\n                            <div className=\"flex-row space-between\">\r\n                                <div className=\"select-tamanho\">\r\n                                    <label> Categoria:</label>\r\n                                    <select className=\"select\">\r\n                                        <option> Vestimenta </option>\r\n                                    </select>\r\n                                </div>\r\n\r\n                                <div className=\"select-tamanho\">\r\n                                    <label> Tema: </label>\r\n                                    <select className=\"select\">\r\n                                        <option> Harry Potter </option>\r\n                                    </select>\r\n                                </div>\r\n                            </div>\r\n\r\n                            \r\n\r\n                            <div className=\"div-array\">\r\n                                <label> Cor </label>\r\n                                <div className=\"flex-row\">\r\n                                    <input type=\"text\" value={cor} onChange={e=> setCor(e.target.value)}className=\"input\"/> <button onClick={SalvarCor} className=\"button-ok\"> OK </button>\r\n                                </div>\r\n                            </div>\r\n\r\n                            <div className=\"div-array\">\r\n                                <label> Tamanhos </label>\r\n                                <div className=\"flex-row\">\r\n                                    <input  className=\"input\"/> <button className=\"button-ok\"> OK </button>\r\n                                </div>  \r\n                            </div>\r\n\r\n                            <div className=\"flex-column\"> \r\n                                <label > Valor </label>\r\n                                <input type=\"text\" value={preco} onChange={e=> setPreco(e.target.value)} className=\"input\"/>\r\n                            </div>\r\n\r\n                            <div>\r\n                                    <div> <label type=\"checked\" value={disponivel} onChange={e=> setDisponivel(e.target.checked)}>Disponível ?</label> <input type='checkbox'/> </div>\r\n                            </div> \r\n                        </div>\r\n\r\n                        <div className=\"div2\">\r\n                            <div>\r\n                                <label> Foto de destaque</label>\r\n                                <input/>\r\n                            </div>\r\n\r\n                            <div> \r\n                                <label> Fotos extras </label>\r\n                                <input/>\r\n                            </div>\r\n                        </div>\r\n\r\n\r\n                    </div>\r\n\r\n                    <button onClick={Adicionar} className=\"button centralizar\"> Adicionar </button>\r\n\r\n                </div>\r\n\r\n                \r\n        </main>\r\n\r\n    )\r\n}","C:\\Users\\Lenovo\\TECH GIRLS\\TCC_TECHGIRLS-1\\site\\src\\components\\Inputs.js",[],[],"C:\\Users\\Lenovo\\TECH GIRLS\\TCC_TECHGIRLS-1\\site\\src\\components\\pagAdm.js",["70"],[],"import { Link } from \"react-router-dom\";\r\nimport './pagAdm.scss';\r\n\r\nexport default function MenuAdmin(props){\r\n    return(\r\n            <div className=\"menu\">\r\n                <img src={props.logo}></img>\r\n                <div>\r\n                    <Link className=\"opcao\">Administrador</Link>\r\n                </div>\r\n                <div>\r\n                  <Link className=\"opcao\">Catálogos</Link>\r\n                </div>\r\n                <div>\r\n                    <Link className=\"opcao\">Pedidos</Link>\r\n                </div>\r\n                <div>\r\n                <Link to='/P